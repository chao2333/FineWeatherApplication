<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="175dp"

    tools:ignore="MissingConstraints" >

    <ImageView
        android:id="@+id/main_sunandrise11"
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="47dp"
        android:src="@drawable/ic_sunrise"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/main_sunandrise12"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="47dp"
        android:layout_toEndOf="@+id/main_sunandrise11"
        android:text="@string/main_sunriseandsetrise"
        android:textColor="@color/mainsunriseandset" />

    <TextView
        android:id="@+id/main_sunandrise19"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="47dp"
        android:layout_toEndOf="@+id/main_sunandrise12"
        android:text="05:00"
        android:textColor="@color/mainsunriseandset" />

    <ImageView
        android:id="@+id/main_sunandrise15"
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="47dp"
        android:layout_toEndOf="@+id/main_sunandrise19"
        android:src="@drawable/ic_sunset"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/main_sunandrise16"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="47dp"
        android:layout_toEndOf="@+id/main_sunandrise15"
        android:text="@string/main_sunriseandsetset"
        android:textColor="@color/mainsunriseandset" />

    <TextView
        android:id="@+id/main_sunandrise20"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="47dp"
        android:layout_toEndOf="@+id/main_sunandrise16"
        android:text="05:00"
        android:textColor="@color/mainsunriseandset" />

    <RelativeLayout
        android:id="@+id/main_sunRiseLayout1"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="75dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="22dp"
        android:layout_marginBottom="20dp">

        <TextView
            android:id="@+id/main_sunandrise21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="0dp"
            android:gravity="center_horizontal"
            android:textColor="@color/mainsunriseandset2"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/main_sunandrise22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/main_sunandrise21"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="10dp"
            android:gravity="center_horizontal"
            android:textColor="@color/white"
            android:textSize="20sp" />
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/main_sunRiseLayout2"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="75dp"
        android:layout_marginBottom="20dp"
        android:layout_toEndOf="@+id/main_sunRiseLayout1">

        <TextView
            android:id="@+id/main_sunandrise23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="0dp"
            android:gravity="center_horizontal"
            android:text="@string/main_sunriseandsethumidity"
            android:textColor="@color/mainsunriseandset2"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/main_sunandrise24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/main_sunandrise23"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="10dp"
            android:gravity="center_horizontal"
            android:textColor="@color/white"
            android:textSize="20sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/main_sunRiseLayout3"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="75dp"
        android:layout_marginBottom="20dp"
        android:layout_toEndOf="@+id/main_sunRiseLayout2">

        <TextView
            android:id="@+id/main_sunandrise25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="0dp"
            android:gravity="center_horizontal"
            android:text="@string/main_sunriseandset2"
            android:textColor="@color/mainsunriseandset2"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/main_sunandrise26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/main_sunandrise25"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="10dp"
            android:layout_alignParentEnd="true"
            android:gravity="center_horizontal"
            android:textColor="@color/white"
            android:textSize="20sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/main_sunRiseLayout4"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="75dp"
        android:layout_marginBottom="20dp"
        android:layout_toEndOf="@+id/main_sunRiseLayout3">

        <TextView
            android:id="@+id/main_sunandrise27"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="0dp"
            android:text="@string/main_sunriseandset3"
            android:gravity="center_horizontal"
            android:textColor="@color/mainsunriseandset2"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/main_sunandrise28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/main_sunandrise27"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="10dp"
            android:gravity="center_horizontal"
            android:textColor="@color/white"
            android:textSize="20sp" />
    </RelativeLayout>

</RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>