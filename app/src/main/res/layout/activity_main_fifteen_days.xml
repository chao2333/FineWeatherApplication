<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="17dp"
    android:background="@color/white1"
    tools:context=".ui.MainAirQualityActivity">

    <RelativeLayout
        android:id="@+id/Location_back21"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/backMainActivity6"
            android:layout_width="23dp"
            android:layout_height="23dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="25dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="1dp"
            android:background="@drawable/iconlocationback" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/LocationTop"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/Location_back21">

        <TextView
            android:id="@+id/Airquality_TopTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginStart="25dp"
            android:layout_marginTop="20dp"
            android:text="@string/fifteendays_title"
            android:textColor="@color/Welcomewhileblack"
            android:textSize="32sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:background="@color/white1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LocationTop">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/fifteenday_recyclerlist"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="3dp"
            android:background="@color/white1"
            android:overScrollMode="never"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="380dp"
        android:layout_height="150dp"
        android:layout_marginTop="32dp"
        android:background="@color/white1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout3" >

        <TextView
            android:id="@+id/fifteenday_detail_date"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="36dp"
            android:layout_marginTop="17dp"
            android:textSize="15sp" />

        <RelativeLayout
            android:id="@+id/fifteenDay_detail_layout"
            android:layout_width="95dp"
            android:layout_height="105dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginBottom="0dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="22dp"
                android:gravity="center_horizontal"
                android:text="@string/main_sunriseandsethumidity"
                android:textColor="@color/Fifteendays_detail_humidity"
                android:textSize="15sp" />
            <TextView
                android:id="@+id/fifteenday_detail_humidity"
                android:gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="60dp"
                android:textColor="@color/DayBlackNightWhile"
                android:textSize="20sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/fifteenDay_detail_layout2"
            android:layout_width="95dp"
            android:layout_height="105dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginBottom="0dp"
            android:layout_toEndOf="@+id/fifteenDay_detail_layout">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="22dp"
                android:gravity="center_horizontal"
                android:text="@string/fifteendays_detail_visibility"
                android:textColor="@color/Fifteendays_detail_humidity"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/fifteenday_detail_visibility"
                android:gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="60dp"
                android:textColor="@color/DayBlackNightWhile"
                android:textSize="20sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/fifteenDay_detail_layout3"
            android:layout_width="95dp"
            android:layout_height="105dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginBottom="0dp"
            android:layout_toEndOf="@+id/fifteenDay_detail_layout2">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="22dp"
                android:gravity="center_horizontal"
                android:text="@string/main_sunriseandset3"
                android:textColor="@color/Fifteendays_detail_humidity"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/fifteenday_detail_pressure"
                android:gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:maxLines="1"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="60dp"
                android:textColor="@color/DayBlackNightWhile"
                android:textSize="20sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/fifteenDay_detail_layout4"
            android:layout_width="95dp"
            android:layout_height="105dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginBottom="0dp"
            android:layout_toEndOf="@+id/fifteenDay_detail_layout3">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="22dp"
                android:gravity="center_horizontal"
                android:text="@string/fifteendays_detail_cloudrate"
                android:textColor="@color/Fifteendays_detail_humidity"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/fifteenday_detail_cloudrate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:maxLines="1"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="60dp"
                android:textColor="@color/DayBlackNightWhile"
                android:textSize="20sp" />
        </RelativeLayout>
    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>